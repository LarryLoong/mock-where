{
    "name": "mock-where",
    "version": "0.1.0",
    "description": "Flexible mock server for RESTful API testing, with a SQL-like mock rule engine",
    "main": "src/app.js",
    "scripts": {
        "start": "node ./src/app.js | bunyan",
        "test": "NODE_ENV=local node_modules/.bin/mocha -c -u exports ./test/ --bail --recursive --harmony -t 10000",
        "lint": "./node_modules/.bin/eslint -f stylish --cache --cache-location ./build/eslint/cache/ --color ./src/",
        "watch": "./node_modules/.bin/esw --watch -f stylish --cache --cache-location ./build/eslint/cache/ --color ./src/"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:qiangyt/mock-where.git"
    },
    "author": {
        "name": "Qiang Yiting",
        "email": "qiangyt@wxcount.com",
        "url": "https://github.com/qiangyt"
    },
    "keywords": [
        "API",
        "http",
        "mock",
        "RESTful",
        "test",
        "web"
    ],
    "license": "MIT",
    "dependencies": {
        "alasql": "^0.4.1",
        "ejs": "^2.5.6",
        "handlebars": "^4.0.10",
        "koa": "2.3.0",
        "koa-body": "2.3.0",
        "koa-logger": "3.0.1",
        "koa-router": "7.2.1",
        "lodash": "^4.17.4",
        "log4js": "^2.3.0",
        "mustache": "^2.3.0",
        "npm": "^5.3.0",
        "sprintf-js": "^1.1.1",
        "underscore": "^1.8.3"
    },
    "devDependencies": {
        "babel": "^6.23.0",
        "babel-cli": "^6.24.1",
        "babel-eslint": "^7.2.3",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "eslint": "^4.3.0",
        "eslint-stylish": "^0.2.0",
        "eslint-watch": "^3.1.2",
        "sinon": "^2.4.1"
    }
}