{
    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "launch",
            "program": "${workspaceRoot}/src/App.js"
        },

        {
            "name": "test",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/jasmine/bin/jasmine.js",
            "stopOnEntry": false,
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "args": [
                "--config=${workspaceRoot}/spec/support/jasmine.json"
            ],
            "runtimeArgs": [
                "--nolazy"
            ],
            "sourceMaps": false,
            "outDir": null
        }

    ]
}